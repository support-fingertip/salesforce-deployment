<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Unit_Price__c</fullName>
    <externalId>false</externalId>
    <formula>Unit_Base_Price__c + IF(ISBLANK(Car_Parking_Price__c),0,Car_Parking_Price__c) + IF(ISBLANK(Additional_Charges__c),0,Additional_Charges__c) - IF(ISBLANK(Discount_Value__c),0,Discount_Value__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Unit Price</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
